{
  "name": "@numl-react/core",
  "version": "0.1.0",
  "description": "React wrappers and components for Numl.Design",
  "scripts": {
    "prettier": "prettier --write ./packages",
    "dev": "vite",
    "clean": "npm-run-all clean:*",
    "clean:dist": "npx lerna exec -- rm ./dist -rf",
    "install:all": "npx lerna exec -- npm install",
    "typedoc": "npx typedoc --out docs packages/entry.ts",
    "build": "npx lerna exec -- npm run build && npx tsc",
    "test": "jest",
    "release": "npm run build && changeset publish",
    "test:watch": "jest --watch",
    "serve": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numldesign/numl-react.git"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "./*",
    "packages/*",
    "packages/Atoms/*",
    "packages/Molecules/*",
    "packages/Elements/*"
  ],
  "main": "./dist/core.cjs.js",
  "module": "./dist/core.es.js",
  "typings": "./dist/typings/entry.d.ts",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@changesets/cli": "^2.16.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-url": "^6.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.1",
    "@semantic-release/npm": "^7.1.1",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.23",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.6.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jsx-native-events": "^2.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^17.0.1",
    "react-test-library": "^0.0.1",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.4",
    "typedoc": "^0.20.36",
    "typescript": "4.2.4",
    "vite": "^2.1.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "design-system",
    "react-components",
    "components",
    "react",
    "numl-react",
    "accessible",
    "ui-library",
    "uikit"
  ],
  "author": "Numl.Design Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/numldesign/numl-react/issues"
  },
  "homepage": "https://github.com/numldesign/numl-react#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write ./packages --ignore-unknown"
  },
  "gitHead": "3f2d9bc03b831b7d51e0fe93290501a7ba44153a",
  "dependencies": {
    "uuidv4": "^6.2.7"
  },
  "dependenciesMeta": {
    "open@7.4.2": {
      "unplugged": true
    }
  }
}
