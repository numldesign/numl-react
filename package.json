{
  "name": "numl-react",
  "version": "0.0.1",
  "private": true,
  "description": "React wrappers and components for Numl.Design",
  "keywords": [
    "design-system",
    "react-components",
    "components",
    "react",
    "numl-react",
    "accessible",
    "ui-library",
    "uikit"
  ],
  "homepage": "https://github.com/numldesign/numl-react#readme",
  "bugs": {
    "url": "https://github.com/numldesign/numl-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numldesign/numl-react.git"
  },
  "license": "MIT",
  "author": "Numl.Design Team",
  "workspaces": [
    "packages/Elements",
    "packages/Utilities",
    "packages/Atoms",
    "packages/Core",
    "packages/Organisms",
    "packages/Molecules",
    "packages/Atoms/*",
    "packages/Core/*",
    "packages/Molecules/*",
    "packages/Utilities/*",
    "packages/Organisms/*"
  ],
  "scripts": {
    "commit": "yarn run prettier && cz",
    "local": "verdaccio --listen 4000 --config ./.verdaccio.yaml",
    "pkg": "manypkg run",
    "check:pkgs": "manypkg check",
    "fix:pkgs": "manypkg fix",
    "prettier": "prettier --write packages",
    "prettier:check": "prettier --check packages",
    "install:all": "npx lerna exec -- yarn install --frozen-lock",
    "install:all:dev": "npx lerna exec -- yarn install",
    "typedoc": "typedoc --out docs packages/entry.ts",
    "build": "npx lerna exec -- yarn build",
    "build:types": "npx lerna exec -- yarn build:types",
    "test": "npx jest",
    "release": "npx lerna exec -- yarn build && changeset publish",
    "test:watch": "jest --watch",
    "serve": "vite preview",
    "eslint:fix": "eslint --fix packages/* --quiet",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/core": "7.14.2",
    "@babel/plugin-transform-typescript": "7.13.0",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.10.4",
    "@changesets/changelog-github": "0.4.0",
    "@changesets/cli": "2.16.0",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@commitlint/config-lerna-scopes": "12.1.1",
    "@manypkg/cli": "0.17.0",
    "@rollup/plugin-babel": "5.1.0",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-url": "6.0.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.1",
    "@semantic-release/npm": "7.1.1",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@storybook/addon-a11y": "6.2.9",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-contexts": "^5.3.20",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-knobs": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/builder-webpack5": "6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "6.2.9",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.6",
    "@types/jest": "26.0.23",
    "@types/mocha": "8.2.2",
    "@types/node": "15.3.0",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.5",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "@vitejs/plugin-react-refresh": "1.3.1",
    "babel-loader": "8.2.2",
    "chromatic": "5.6.1",
    "commitlint": "12.1.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.26.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.2.1",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lerna": "3.2.0",
    "lint-staged": "11.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "prop-types": "15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.2",
    "react-test-library": "0.0.1",
    "semantic-release": "17.4.2",
    "storybook-addon-themes": "^6.1.0",
    "ts-jest": "26.5.4",
    "typedoc": "0.20.36",
    "typescript": "4.2.4",
    "uuid": "^8.3.2",
    "uuidv4": "6.2.7",
    "verdaccio": "^5.1.0",
    "vite": "2.2.4"
  },
  "dependenciesMeta": {
    "open@7.4.2": {
      "unplugged": true
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "gitHead": "3f2d9bc03b831b7d51e0fe93290501a7ba44153a"
}
